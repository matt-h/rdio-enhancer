{
	"manifest_version": 2,
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "rdio-enhancer.js" ],
      "css": [ "rdio-enhancer.css" ],
      "matches": [ "*://www.rdio.com/*" ],
      "run_at": "document_end"
   } ],
   "web_accessible_resources": ["options.html"],
   "description": "Enhances rdio.com with many of the most requested Feature Requests for rdio. View details for full list.",
   "name": "Rdio Enhancer",
	"permissions": [
		"notifications"
	],
   "icons": {
   	"48": "48.png",
   	"128": "128.png"
   },
   "version": "2.9"
}
